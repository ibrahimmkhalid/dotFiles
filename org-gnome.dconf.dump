[desktop/interface]
clock-format='12h'
cursor-theme='Yaru'
document-font-name='Ubuntu 10'
enable-hot-corners=true
font-antialiasing='grayscale'
font-hinting='slight'
font-name='Ubuntu 10'
gtk-im-module='gtk-im-context-simple'
gtk-theme='Nordic-Polar-v40'
icon-theme='Numix-Circle-Light'
monospace-font-name='FiraCode Nerd Font 11'
show-battery-percentage=true
toolkit-accessibility=false

[desktop/peripherals/keyboard]
numlock-state=false

[desktop/privacy]
disable-microphone=false
report-technical-problems=true

[desktop/wm/preferences]
button-layout='appmenu:minimize,maximize,close'
num-workspaces=15
titlebar-font='Ubuntu 10'

[gnome-screenshot]
border-effect='none'
delay=0
include-border=true
include-pointer=false

[gnome-system-monitor]
current-tab='processes'
maximized=true
network-total-in-bits=false
show-dependencies=false
show-whose-processes='user'
window-state=(1920, 1055, 0, 25)

[nautilus/preferences]
default-folder-viewer='list-view'

[shell]
disable-user-extensions=false
enabled-extensions=['user-theme@gnome-shell-extensions.gcampax.github.com', 'clipboard-indicator@tudmotu.com', 'caffeine@patapon.info', 'appindicatorsupport@rgcjonas.gmail.com', 'dash-to-plank@hardpixel.eu', 'dash-to-dock@micxgx.gmail.com', 'todolist@tomMoral.org', 'guillotine@fopdoodle.net']

[shell/extensions/caffeine]
indicator-position-max=2
nightlight-control='never'

[shell/extensions/custom-hot-corners/monitor-0-top-left]
action='toggleOverview'

[shell/extensions/dash-to-dock]
animate-show-apps=true
apply-custom-theme=false
background-opacity=0.80000000000000004
custom-theme-shrink=true
dash-max-icon-size=32
dock-fixed=false
dock-position='BOTTOM'
extend-height=false
force-straight-corner=false
height-fraction=0.90000000000000002
icon-size-fixed=false
preferred-monitor=0
scroll-action='switch-workspace'
show-apps-at-top=false
show-trash=false

[shell/extensions/dash-to-panel]
animate-appicon-hover-animation-extent={'RIPPLE': 4, 'PLANK': 4, 'SIMPLE': 1}
appicon-margin=4
appicon-padding=4
available-monitors=[0]
dot-style-focused='DASHES'
dot-style-unfocused='DOTS'
hotkeys-overlay-combo='TEMPORARILY'
isolate-workspaces=true
leftbox-padding=-1
panel-anchors='{"0":"MIDDLE"}'
panel-element-positions='{"0":[{"element":"showAppsButton","visible":true,"position":"stackedTL"},{"element":"activitiesButton","visible":false,"position":"stackedTL"},{"element":"leftBox","visible":true,"position":"stackedTL"},{"element":"taskbar","visible":true,"position":"stackedTL"},{"element":"centerBox","visible":true,"position":"stackedBR"},{"element":"rightBox","visible":true,"position":"stackedBR"},{"element":"dateMenu","visible":true,"position":"stackedBR"},{"element":"systemMenu","visible":true,"position":"stackedBR"},{"element":"desktopButton","visible":true,"position":"stackedBR"}]}'
panel-lengths='{"0":100}'
panel-positions='{"0":"BOTTOM"}'
panel-sizes='{"0":36}'
show-apps-icon-file=''
status-icon-padding=-1
tray-padding=-1
tray-size=0
window-preview-title-position='TOP'

[shell/extensions/desktop-icons]
show-home=false
show-trash=false

[shell/extensions/executor]
center-active=false
center-index=1
left-active=false
location=1

[shell/extensions/user-theme]
name='Nordic-Polar-v40'
